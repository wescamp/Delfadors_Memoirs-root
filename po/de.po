#: Delfadors_Memoirs/scenarios/council_in_weldyn.cfg:3
msgid "Council in Weldyn"
msgstr "Der Rat in Weldyn"

#: Delfadors_Memoirs/scenarios/land_of_the_dead.cfg:3
msgid "Land of the Dead"
msgstr "Das Land der Toten"

#: Delfadors_Memoirs/scenarios/land_of_the_dead.cfg:53
msgid "Defeat Ihiah-Malal"
msgstr "Besiegt Ihiah-Malal"

#: Delfadors_Memoirs/scenarios/swamps_of_illuven.cfg:109
#: Delfadors_Memoirs/scenarios/land_of_the_dead.cfg:57
#: Delfadors_Memoirs/scenarios/wasteland.cfg:72
#: Delfadors_Memoirs/scenarios/road_to_weldyn.cfg:83
#: Delfadors_Memoirs/scenarios/land_of_the_dead_intro.cfg:215
